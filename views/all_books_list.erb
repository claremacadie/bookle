<table>
  <thead>
    <tr>
      <th scope = "col"> Title </th>
      <th scope = "col"> Collection </th>
      <th scope = "col"> Author(s) </th>
      <th scope = "col"> Categories </th>
      <th scope = "col"> Owners </th>
    </tr>
  </thead>
  <tbody>
    <% @books.each do |book| %>
      <tr>
        <td> <%= book[:title] %></td>
        <td> <%= book[:collection] %></td>
        <td> <%= book[:authors] %></td>
        <td> <%= book[:categories] %></td>
        <td> <%= book[:owners] %></td>
      </tr>
    <% end %>
  </tbody>
</table>
